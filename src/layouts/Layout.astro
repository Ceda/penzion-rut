---
interface Props {
  title?: string;
  description?: string;
  keywords?: string;
}

const {
  title = "Máchovo jezero ubytování - Pension RUT, Máchovo jezero",
  description = "Pension se nachází v lázeňském městečku Staré Splavy, které jsou známé ozdravným vzduchem. Jedinečnost objektu je mimo jiné dána jeho romantickým umístěním nad Máchovým jezerem.",
  keywords = "Ubytování Máchovo jezero, dovolená, Staré Splavy",
} = Astro.props;
---

<!doctype html>
<html>
  <head>
    <title>{title}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta
      name="google-site-verification"
      content="ib0MMyZ0nMYt6ogb31WhmOCDK3x9BZQOno3zaWLrUIA"
    />
    <meta name="keywords" content={keywords} />
    <meta name="description" content={description} />
    <link
      rel="stylesheet"
      type="text/css"
      href="/theme/pensionrut/css/bootstrap.min.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="/theme/pensionrut/css/bootstrap-responsive.min.css"
    />
    <link
      rel="stylesheet"
      type="text/css"
      href="/theme/pensionrut/css/style.min.css"
    />
    <script type="text/javascript" src="/theme/pensionrut/js/bootstrap.min.js"
    ></script>
    <script type="text/javascript" src="/theme/pensionrut/js/scripts.min.js"
    ></script>
  </head>
  <body>
    <!-- Navbar
  ============================== -->
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <button
            data-target=".nav-collapse"
            data-toggle="collapse"
            class="btn btn-navbar"
            type="button"
          >
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a href="/" class="brand">
            <img src="/theme/pensionrut/img/mini-logo.png" alt="Pension RUT" />
          </a>
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li><a href="/">Pension RUT</a></li><li>
                <a href="/historie">Historie</a>
              </li><li><a href="/ubytovani">Ubytování</a></li><li>
                <a href="/okoli">Okolí</a>
              </li><li><a href="/vylety">Výlety</a></li><li>
                <a href="/cenik">Ceník</a>
              </li><li><a href="/fotogalerie">Fotogalerie</a></li><li>
                <a href="/kontakty">Kontakty</a>
              </li><li><a href="/regata-rut-classic">Regata RUT Classic</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>

    <!-- Header
  ============================== -->
    <header id="overview" class="">
      <div class="container">
        <div class="row-fluid">
          <div class="span6">
            <a href="/">
              <img
                src="/theme/pensionrut/img/header-logo.jpg"
                alt="Header Logo"
                width="435"
                height="82"
              />
            </a>
          </div>
          <div class="span6">
            <img
              src="/theme/pensionrut/img/header-text.jpg"
              alt="Header Text"
              width="272"
              height="45"
              class="headertext"
            />
          </div>
        </div>
      </div>
    </header>

    <!-- Slider
  ============================== -->

    <div class="slider">
      <div id="leftBarArrow" class="barArrow"></div>
      <div id="rightBarArrow" class="barArrow"></div>
      <div class="iosSlider">
        <div class="islider">
          <div class="item">
            <div class="caption">
              <span
                ><a href="/"
                  ><strong>Pension RUT</strong> | Ubytování u Máchova jezera</a
                ></span
              >
              <div class="bg"></div>
            </div>
            <a href="/"
              ><img src="/theme/pensionrut/img/galerie/terasa.jpg" alt="" /></a
            >
          </div>
          <div class="item">
            <div class="caption">
              <span
                ><a href="/historie"
                  ><strong>Historie</strong> | Pensionu RUT a rodiny Formanů</a
                ></span
              >
              <div class="bg"></div>
            </div>
            <a href="/historie"
              ><img
                src="/theme/pensionrut/img/galerie/rok 1926.jpg"
                alt=""
              /></a
            >
          </div>
          <div class="item">
            <div class="caption">
              <span
                ><a href="/ubytovani"
                  ><strong>Ubytování</strong> | Pension RUT ve Starých Splavech</a
                ></span
              >
              <div class="bg"></div>
            </div>
            <a href="/ubytovani"
              ><img
                src="/theme/pensionrut/img/galerie/pokoj 14.jpg"
                alt=""
              /></a
            >
          </div>
          <div class="item">
            <div class="caption">
              <span
                ><a href="/okoli"
                  ><strong>Okolí</strong> | Krásná krajina Máchova jezera</a
                ></span
              >
              <div class="bg"></div>
            </div>
            <a href="/okoli"
              ><img src="/theme/pensionrut/img/galerie/okoli.jpg" alt="" /></a
            >
          </div>
          <div class="item">
            <div class="caption">
              <span
                ><a href="/regata-rut-classic"
                  ><strong>Regata RUT Classic</strong> | Pohár Withe Queen</a
                ></span
              >
              <div class="bg"></div>
            </div>
            <a href="/regata-rut-classic"
              ><img
                src="/theme/pensionrut/img/galerie/rut-classic.jpg"
                alt=""
              /></a
            >
          </div>
          <div class="item">
            <div class="caption">
              <span
                ><a href="/vylety"
                  ><strong>Výlety</strong> | Poznejte krásu Máchova jezera</a
                ></span
              >
              <div class="bg"></div>
            </div>
            <a href="/vylety"
              ><img src="/theme/pensionrut/img/galerie/bezdez.jpg" alt="" /></a
            >
          </div>
        </div>
      </div>
    </div>

    <!-- Content
  ============================== -->
    <div id="body">
      <div class="container">
        <div class="row-fluid">
          <div class="span8">
            <slot />
          </div>

          <!-- Aktuality
        ============================== -->
          <div class="span4">
            <h3>Aktuality<small> od Máchova jezera</small></h3>
            <div style="height: 2px"></div>
            <behold-widget feed-id="CvncMIYMk2n1PeSj4r20"></behold-widget>
            <div style="height: 5px"></div>
            <h3>Máchovo jezero <small>počasí</small></h3>
            <div class="weather_info">
              <p class="weather-col">
                <img
                  title="déšť"
                  src="https://openweathermap.org/img/wn/10d@2x.png"
                /><span class="date"
                  ><strong>čtvrtek</strong> <strong>23 říj</strong>
                </span><span class="info"
                  ><span>Min:&nbsp;8°C</span> <span>Max:&nbsp;16°C</span></span>
              </p><p class="weather-col">
                <img
                  title="slabý déšť"
                  src="https://openweathermap.org/img/wn/10d@2x.png"
                /><span class="date"
                  ><strong>pátek</strong> <strong>24 říj</strong>
                </span><span class="info"
                  ><span>Min:&nbsp;5°C</span> <span>Max:&nbsp;9°C</span></span>
              </p><p class="weather-col">
                <img
                  title="slabý déšť"
                  src="https://openweathermap.org/img/wn/10d@2x.png"
                /><span class="date"
                  ><strong>sobota</strong> <strong>25 říj</strong>
                </span><span class="info"
                  ><span>Min:&nbsp;5°C</span> <span>Max:&nbsp;8°C</span></span>
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Footer
  ============================== -->
    <footer>
      <div class="container">
        <div class="row-fluid">
          <div class="span8">
            <p>
              Copyright © 2013 Pension RUT - Nejstarší pension na břehu Máchova
              jezera
            </p>
          </div>
          <div class="span4">
            <p>
              Created with pleasure by <a href="http://ceda.splavy.com"
                >Ceda_</a>
            </p>
          </div>
        </div>
      </div>
    </footer>

